{
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint"],
    "env": {
        "browser": true
    },
    "rules": {
        "indent": [
            "error",
            4
        ],
        "max-len": 0,
        "no-underscore-dangle": 0,
        "react/jsx-indent": [
            "error",
            4
        ],
        "react/jsx-indent-props": [
            "error",
            4
        ],
        "@typescript-eslint/indent": [
            "error",
            4
        ]
    },
    "extends": "airbnb-typescript",
    "overrides": [
        {
            "files": "**/**.ts"
        }
    ]
}
