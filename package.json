{
    "name": "newbuild",
    "version": "1.0.0",
    "description": "New Build",
    "main": "index.js",
    "scripts": {
        "build": "cross-env NODE_ENV=development webpack --config ./webpack.config.js",
        "build:prod": "cross-env NODE_ENV=production webpack --config ./webpack.config.js",
        "docs": "documentation build ./src > ./docs/codebase.md --shallow false --format md --sort-order alpha",
        "release": "standard-version && node ./tasks/version.js",
        "start": "cross-env NODE_ENV=development webpack-dev-server --content-base ./ --config  ./webpack.config.js --open true --port 9001 --inline",
        "test": "jest",
        "lint": "eslint ./src/ --cache --ignore-pattern .gitignore"
    },
    "author": "Franklin Waller",
    "license": "MIT",
    "devDependencies": {
        "@types/react": "^16.4.6",
        "@types/react-dom": "^16.0.6",
        "@types/react-redux": "^6.0.3",
        "@types/react-router": "^4.0.28",
        "@types/react-router-redux": "^5.0.15",
        "babel-core": "6.26.3",
        "babel-eslint": "8.2.5",
        "babel-loader": "7.1.4",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-plugin-transform-es3-member-expression-literals": "^6.22.0",
        "babel-plugin-transform-es3-property-literals": "^6.22.0",
        "babel-preset-env": "1.7.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "babel-runtime": "^6.26.0",
        "cross-env": "5.2.0",
        "css-loader": "^0.28.11",
        "documentation": "8.0.0",
        "eslint": "5.0.1",
        "eslint-config-airbnb": "^17.0.0",
        "eslint-loader": "^2.0.0",
        "eslint-plugin-babel": "5.1.0",
        "eslint-plugin-import": "^2.13.0",
        "eslint-plugin-jsx-a11y": "^6.0.3",
        "eslint-plugin-react": "^7.10.0",
        "eslint-summary": "^1.0.0",
        "node-sass": "^4.9.0",
        "sass-loader": "^7.0.3",
        "standard-version": "^4.4.0",
        "style-loader": "^0.21.0",
        "ts-loader": "^4.4.2",
        "typescript": "^2.9.2",
        "webpack": "^4.14.0",
        "webpack-cli": "^3.0.8",
        "webpack-dev-server": "3.1.4",
        "jest": "^23.2.0"
    },
    "dependencies": {
        "babel-polyfill": "6.26.0",
        "react": "^16.4.1",
        "react-dom": "^16.4.1",
        "whatwg-fetch": "2.0.4"
    }
}
