{
  "name": "newbuild",
  "version": "1.1.0",
  "description": "New Build",
  "main": "index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=development webpack --config ./webpack.config.js",
    "build:prod": "cross-env NODE_ENV=production webpack --config ./webpack.config.js",
    "docs": "documentation build ./src > ./docs/codebase.md --shallow false --format md --sort-order alpha",
    "release": "standard-version && node ./tasks/version.js",
    "start": "cross-env NODE_ENV=development webpack-dev-server --content-base ./ --config  ./webpack.config.js --open true --port 9001 --inline",
    "test": "jest",
    "lint": "eslint ./src/ --cache --ignore-pattern .gitignore"
  },
  "author": "Franklin Waller",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.2",
    "@types/react-router": "^5.0.3",
    "@types/react-router-redux": "^5.0.18",
    "babel-core": "6.26.3",
    "babel-eslint": "10.0.2",
    "babel-loader": "8.0.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es3-member-expression-literals": "^6.22.0",
    "babel-plugin-transform-es3-property-literals": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "cross-env": "5.2.0",
    "css-loader": "^3.2.0",
    "documentation": "12.1.1",
    "eslint": "6.2.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-summary": "^1.0.0",
    "jest": "^24.9.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.2.0",
    "standard-version": "^7.0.0",
    "style-loader": "^1.0.0",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "3.8.0"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-polyfill": "6.26.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "whatwg-fetch": "3.0.0"
  }
}
